{
  "name": "typescript-action",
  "version": "0.0.0",
  "description": "GitHub Action for complex pull request approval cases",
  "author": "Parity <admin@parity.io> (https://parity.io)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paritytech/pr-custom-review.git"
  },
  "license": "MIT",
  "keywords": [
    "actions",
    "node"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "package": "ncc --source-map --license licenses.txt build build/main.js",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "@octokit/webhooks-types": "^4.0.3",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/node": "^15.12.5",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-compactify": "^0.1.5",
    "ts-jest": "^27.0.0",
    "typescript": "^4.3.5"
  },
  "bugs": {
    "url": "https://github.com/paritytech/pr-custom-review/issues"
  },
  "homepage": "https://github.com/paritytech/pr-custom-review#readme",
  "main": ".eslintrc.js"
}
